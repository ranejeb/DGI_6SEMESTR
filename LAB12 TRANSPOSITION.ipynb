    "def encode(txt,key):\n",
    "    sz = len(key)  # how big are the columns \n",
    "    cols = list(map(\"\".join,zip(*zip(*[iter(txt)]*sz)))) # list partitioned into columns\n",
    "    return \"\".join([cols[key.index(str(c))] for c in range(1,sz+1)])\n",
    "\n",
    "\n",
    "\n",
    "encoded = encode(\"IHAVETWOCATS\",\"3124\")\n",
    "print encoded"
   ]
